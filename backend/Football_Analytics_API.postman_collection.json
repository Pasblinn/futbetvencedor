		{
			"name": "üìä Test Scenarios",
			"item": [
				{
					"name": "Premier League Derby Test",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has prediction data\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('match_outcome');",
									"    pm.expect(jsonData).to.have.property('goals_prediction');",
									"    pm.expect(jsonData).to.have.property('confidence_system');",
									"});",
									"",
									"pm.test(\"Confidence system is working\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.confidence_system).to.have.property('overall_confidence');",
									"    pm.expect(jsonData.confidence_system.overall_confidence).to.be.a('number');",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "accept",
								"value": "application/json"
							}
						],
						"url": {
							"raw": "{{base_url}}{{api_version}}/predictions/test-live-engine/64/65",
							"host": ["{{base_url}}"],
							"path": ["{{api_version}}", "predictions", "test-live-engine", "64", "65"]
						}
					},
					"response": []
				},
				{
					"name": "ML Enhanced Prediction Test",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Enhanced prediction structure\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('status', 'success');",
									"    pm.expect(jsonData).to.have.property('data');",
									"    pm.expect(jsonData.data).to.have.property('predictions');",
									"});",
									"",
									"pm.test(\"System status included\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.data).to.have.property('system_status');",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "accept",
								"value": "application/json"
							}
						],
						"url": {
